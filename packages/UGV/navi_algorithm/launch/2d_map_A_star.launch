<launch>
    <!-- 启动二维地图 -->
    <node pkg="navi_algorithm" type="2d_map_creater" name="map_creater_node" >
        <param name="obstacle_num" value="60" />
        <param name="obstacle_len" value="20" />
    </node>

    <!--A star 算法-->
    <node pkg="navi_algorithm" type="A_star_search" name="A_stars_search" output="screen">
        <param name="heuristic_func" value="0" />  <!--启发函数选择 0 - Manhattan Distance 1 - Diagonal distance 2 - Euclidean distance -->
        <param name="iter_step" value="500" /> <!-- 为了更好的显示，每隔iter_step步，需要键入值以继续 -->
    </node>
    
     <!-- RViz -->
    <node pkg="rviz" type="rviz" name="$(anon rviz)" respawn="false" output="screen" args="-d $(find navi_algorithm)/rviz/2d_map.rviz"/>


</launch>